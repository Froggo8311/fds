#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/BrowserLinux/node_modules/.pnpm/sorcery@0.10.0/node_modules/sorcery/bin/node_modules:/home/runner/BrowserLinux/node_modules/.pnpm/sorcery@0.10.0/node_modules/sorcery/node_modules:/home/runner/BrowserLinux/node_modules/.pnpm/sorcery@0.10.0/node_modules:/home/runner/BrowserLinux/node_modules/.pnpm/node_modules:/home/runner/BrowserLinux/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/BrowserLinux/node_modules/.pnpm/svelte-preprocess@4.10.4_825cbf44134a6d0c58940e0b0bfe23fc/node_modules/svelte-preprocess/node_modules:/home/runner/BrowserLinux/node_modules/.pnpm/svelte-preprocess@4.10.4_825cbf44134a6d0c58940e0b0bfe23fc/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/runner/BrowserLinux/node_modules/.pnpm/sorcery@0.10.0/node_modules/sorcery/bin/node_modules:/home/runner/BrowserLinux/node_modules/.pnpm/sorcery@0.10.0/node_modules/sorcery/node_modules:/home/runner/BrowserLinux/node_modules/.pnpm/sorcery@0.10.0/node_modules:/home/runner/BrowserLinux/node_modules/.pnpm/node_modules:/home/runner/BrowserLinux/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/BrowserLinux/node_modules/.pnpm/svelte-preprocess@4.10.4_825cbf44134a6d0c58940e0b0bfe23fc/node_modules/svelte-preprocess/node_modules:/home/runner/BrowserLinux/node_modules/.pnpm/svelte-preprocess@4.10.4_825cbf44134a6d0c58940e0b0bfe23fc/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sorcery@0.10.0/node_modules/sorcery/bin/index.js" "$@"
else
  exec node  "$basedir/../../../../../sorcery@0.10.0/node_modules/sorcery/bin/index.js" "$@"
fi
